<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Document</title>
</head>
<body>
    <header>
        <h2>
            All User's 
        </h2>
        <a href="/new">
            <button>Create User</button>
        </a>
    </header>
    <main>
        <section>
            <% users.forEach(user => { %>
            <div>
              <a href="/<%= user.id %>">
                <h2><%= user.name %></h2>
              </a>
            </div>
            <% }) %>
          </section>
      
    </main>
</body>
</html>